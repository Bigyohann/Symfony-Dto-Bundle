{
  "type": "symfony-bundle",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "name": "bigyohann/symfony-dto-bundle",
  "description": "Symfony bundle to add automatic Dto converter",
  "authors": [
    {
      "email": "contact@yohannbiglia.fr",
      "name": "Yohann BIGLIA"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/framework-bundle": ">=6.2",
    "symfony/runtime": "6.*",
    "symfony/serializer": "6.*",
    "symfony/validator": "6.*",
    "symfony/yaml": "6.*",
    "symfony/serializer-pack": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Bigyohann\\DtoBundle\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
